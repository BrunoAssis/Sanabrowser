{
    "name": "Sanabrowser",
    "description": "Troque todas as fotos do seu navegador por este homem charmoso e misterioso.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "assets/images/icon16.png",
        "32": "assets/images/icon32.png",
        "48": "assets/images/icon48.png",
        "128": "assets/images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "assets/js/contentScript.js"
            ]
        }
    ],
    "background": {
        "service_worker": "assets/js/background.js"
    }
}